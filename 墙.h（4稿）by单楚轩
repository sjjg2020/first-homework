#include<iostream> 
using namespace std;
	
class Wall
{public:
	enum
	{
		Row=30,
		Col=30
	};
	void newwall();//刷新 
	void putwall();//输出 
	char Getdata(int a, int b);//获得墙的坐标
	void wallchange(int a,int b,char c);//墙是输出中心 
	char Newwall();//随时间形成的新的障碍 （新） 
	void difficulty(int a) ;//难度设置 (新) 
	
private:
	char wall[Row][Col];//所含字符 
};  
